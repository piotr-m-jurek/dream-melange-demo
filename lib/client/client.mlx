let () =
  let body =
    Webapi.Dom.document
    |> Webapi.Dom.Document.querySelector "#app"
  in
  match body with
  | None -> ()
  | Some body ->
    let text = "Hello from Melange" in
    let p = Webapi.Dom.document
      |> Webapi.Dom.Document.createElement "p"
    in
    text |> Webapi.Dom.Element.setInnerText p;
    body |> Webapi.Dom.Element.appendChild p;

